<FindBugsFilter>
	<Match>
		<Class name="org.eclipselabs.restlet.internal.di.eclipse.bundle.Activator"/>
		<Or>
			<Method name="start"/>
			<Method name="stop"/>
		</Or>
		<Bug code="ST"/>
	</Match>

	<Match>
		<Class name="org.eclipselabs.restlet.developer.junit.RestletTestHarness"/>
		<Method name="setUp"/>
		<Bug code="ST"/>
	</Match>
</FindBugsFilter>